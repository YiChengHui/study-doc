div.customPrint {
    color: #42b983;
    cursor: pointer;
    font-weight: 700;
}

.theme-default-content blockquote {
    color: #000;
    padding: 8px 16px;
    background-color: #f3f5f7;
    border-radius: 4px;
    border-left: 5px solid #42b983;
    margin: 20px 0;
}

.theme-default-content strong {
    color: red;
    font-weight: 700;
}
